{
  "generator-jhipster": {
    "databaseType": "sql",
    "devDatabaseType": "mysql",
    "enableHibernateCache": true,
    "enableSwaggerCodegen": true,
    "enableTranslation": true,
    "jhiPrefix": "jhi",
    "languages": [
      "en",
      "fr"
    ],
    "messageBroker": "kafka",
    "nativeLanguage": "fr",
    "packageName": "com.electron.mfs.pg.transactions",
    "packageFolder": "com/electron/mfs/pg/transactions",
    "prodDatabaseType": "mysql",
    "searchEngine": false,
    "serviceDiscoveryType": "eureka",
    "skipClient": true,
    "testFrameworks": [
      "cucumber"
    ],
    "websocket": false,
    "baseName": "transactionManager",
    "applicationType": "microservice",
    "authenticationType": "uaa",
    "uaaBaseName": "pgkerneluaa",
    "cacheProvider": "hazelcast",
    "buildTool": "maven",
    "serverPort": "21011",
    "jhipsterVersion": "6.1.2",
    "skipUserManagement": true,
    "clientPackageManager": "npm",
    "entitySuffix": "",
    "dtoSuffix": "DTO",
    "otherModules": []
  },
  "entities": [
    "Transaction",
    "DetailTransaction",
    "TransactionInfo",
    "LoanInstalment",
    "TransactionPrice",
    "TransactionCommission",
    "Operation",
    "InternalConnectorRequest",
    "InternalConnectorStatus"
  ]
}